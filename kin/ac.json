{
    "openapi": "3.0.3",
    "info": {
        "version": "1.0.0",
        "title": "OpenAPI for Vault Database Component ac of team admin",
        "description": "HTTP API to fully access to Database.",
        "license": {
            "name": "MIT"
        }
    },
    "servers": [
        {
            "url": "/v1/graph/model/admin/ac",
            "description": "Database Content Engine for component ac of team admin"
        }
    ],
    "paths": { 
        "/": {
            "get": {
                "summary": "List of records in ac",
                "operationId": "listadmin_ac",
                "parameters": [
                    {"$ref": "#/components/parameters/fields"},
                    {"$ref": "#/components/parameters/sortby"},
                    {"$ref": "#/components/parameters/sortreverse"},
                    {"$ref": "#/components/parameters/pagesize"},
                    {"$ref": "#/components/parameters/pageno"},
                    {"$ref": "#/components/parameters/totalno"},
                    {"$ref": "#/components/parameters/maxpageno"},
                    {"$ref": "#/components/parameters/ac_id"},
                    {"$ref": "#/components/parameters/entitytype_id"},
                    {"$ref": "#/components/parameters/entity_id"},
                    {"$ref": "#/components/parameters/othertype_id"},
                    {"$ref": "#/components/parameters/other_id"}
                ],
                "responses": {
                    "200": {
                        "description": "List of records in ac",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/admin_ac"
                                    }
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "unexpected error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/{ac_id}": {
            "get": {
                "summary": "Get a record in ac",
                "operationId": "getadmin_ac",
                "parameters": [
                    {
                        "name": "ac_id",
                        "in": "path",
                        "description": "ID of ac",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Get a record in ac",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/admin_ac"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "unexpected error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "parameters": {
            "fields": {
                "name": "fields",
                "in": "query",
                "description": "Fields to be returned, separated by comma",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "sortby": {
                "name": "sortby",
                "in": "query",
                "description": "Sort by field",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "sortreverse": {
                "name": "sortreverse",
                "in": "query",
                "description": "Sort by field in reverse order",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "pagesize": {
                "name": "pagesize",
                "in": "query",
                "description": "Page size",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "pageno": {
                "name": "pageno",
                "in": "query",
                "description": "Page number",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "totalno": {
                "name": "totalno",
                "in": "query",
                "description": "Total number of records",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "maxpageno": {
                "name": "maxpageno",
                "in": "query",
                "description": "Maximum page number",
                "required": false,
                "schema": {
                    "type": "string"
                }
        	},
           	"ac_id": {
                "name": "ac_id",
                "in": "query",
                "description": "ac_id",
                "required": false,
                "schema": {
                    "type": "integer"
                }
            },
            "entitytype_id": {
                "name": "entitytype_id",
                "in": "query",
                "description": "entitytype_id",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "entity_id": {
                "name": "entity_id",
                "in": "query",
                "description": "entity_id",
                "required": false,
                "schema": {
                    "type": "integer"
                }
            },
            "othertype_id": {
                "name": "othertype_id",
                "in": "query",
                "description": "othertype_id",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "other_id": {
                "name": "other_id",
                "in": "query",
                "description": "other_id",
                "required": false,
                "schema": {
                    "type": "integer"
                }
            }
        },
        "schemas": {
            "admin_acs": {
                "type": "array",
                "items": {
                    "$ref": "#/components/schemas/admin_act"
                }
            },
            "admin_act": {
                "type": "object",
                "properties": {
                "ac_id": {
                        "type": "integer"
                    },
                    "entitytype_id": {
                        "type": "string"
                    },
                    "entity_id": {
                        "type": "integer"
                    },
                    "othertype_id": {
                        "type": "string"
                    },
                    "other_id": {
                        "type": "integer"
                    }
                }
            },                             
            "admin_ac": {
                "type": "object",
                "properties": {
                "ac_id": {
                        "type": "integer"
                    },
                    "entitytype_id": {
                        "type": "string"
                    },
                    "entity_id": {
                        "type": "integer"
                    },
                    "othertype_id": {
                        "type": "string"
                    },
                    "other_id": {
                        "type": "integer"
                    }
                }
            },
            "Error": {
                "required": [
                    "code",
                    "message"
                ],
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "integer"
                    },
                    "message": {
                        "type": "string"
                    }
                }
            }
        },
        "securitySchemes": {
            "vaultToken": {
                "type": "apiKey",
                "in": "header",
                "name": "X-VAULT-TOKEN"
            }
        }
    },
    "security": [
        {"vaultToken": []}
    ]
}
