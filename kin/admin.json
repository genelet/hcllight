    "components": {
        "schemas": {
            "Dashboard": {
                "type": "object",
                "properties": {
                    "landings": {
                        "description": "List of landing components to start with in admin",
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "generics": {
                        "description": "List of generic components in admin",
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }
            },
            "Error": {
                "required": [
                    "code",
                    "message"
                ],
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "integer"
                    },
                    "message": {
                        "type": "string"
                    }
                }
            }
        },
        "securitySchemes": {
            "vaultToken": {
                "type": "apiKey",
                "in": "header",
                "name": "X-VAULT-TOKEN"
            }
        }
    },
    "security": [
